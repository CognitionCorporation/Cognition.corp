		<!-- Display Results -->
		<ul>
			<li dir-paginate="result in results | itemsPerPage: pageSize" current-page="currentPage">{{ result }}</li>
		</ul>
		<dir-pagination-controls 
			boundary-links="true"
			on-page-change="getResults(newPageNumber)" 
			template-url="node_modules/angular-utils-pagination/dirPagination.tpl.html">
		</dir-pagination-controls>
		
		
		
	<script src="node_modules/angular-utils-pagination/dirPagination.js"></script>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
		/*********************
	 * Display Results */
	
	// Init
	$scope.currentPage = 1;
	$scope.pageSize = 8;
	$scope.results = [];
	for(var i = 1; i < 100; i++){
		$scope.results.push(i);
	}
	$scope.newPageNumber = 5;
	
	
	$scope.getResults = function(newPageNumber){
		console.log("getResults(newPageNumber) called");
		//var moreResults = getResultsService.query({file: "Activities.json"});
		var moreResults = [];
		for(result in moreResults){
			$scope.results.push(result.name);
		}
	}
	
	$scope.getResults($scope.pageSize);
	
}]);



FrontEndApp.factory('getResultsService', function ($resource) {
    return $resource('frontEnd/data/:file',{file: "@file"});
});
